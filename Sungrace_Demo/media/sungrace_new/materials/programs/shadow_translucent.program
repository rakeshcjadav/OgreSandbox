vertex_program shadow_translucent_caster_vp glsl
{
    source shadow_translucent_caster_vp.glsl

    default_params
    {
        param_named_auto matWorldView worldview_matrix
		param_named_auto matWorldViewProj worldviewproj_matrix
    }
}

fragment_program shadow_translucent_caster_fp glsl
{
    source shadow_translucent_caster_fp.glsl

    default_params
    {
        param_named_auto depthRange scene_depth_range
    }
}